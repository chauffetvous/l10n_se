            <field name="description">PTE25</field>
            <field name="amount" eval="100.00"/>
            <field name="amount_type">group</field>
            <field name="children_tax_ids" eval="[(6, 0, [ref('tax_25_EU_template_m'), ref('tax_25_EU_template_reverse')])]"/>
            <field name="type_tax_use">purchase</field>
        </record>


    </data>
