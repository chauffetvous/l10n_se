<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Tax template for sale and purchase -->
        <record id="sale_tax_25_template" model="account.tax.template">
            <field name="chart_template_id" ref="l10nse_chart_template"/>
